name = "mesh"
version = "@PACKAGE_VERSION@"
description = "Interface to various mesh generators"
requires = "bigarray"
archive(byte) = "mesh.cma easymesh.cma mesh_triangle.cma"
archive(native) = "mesh.cmxa easymesh.cmxa mesh_triangle.cmxa"

package "display" (
  description = "Display meshes"
  version = "@PACKAGE_VERSION@"
  requires += "graphics"
  archive(byte) += "mesh_display.cma"
  archive(native) += "mesh_display.cmxa"
)
